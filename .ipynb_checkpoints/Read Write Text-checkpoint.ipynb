   "source": [
    "# it is really easy to write to a text file\n",
    "words = 'time for coffee!!!'\n",
    "# we need a file access object\n",
    "fout = open('myfile.txt', 'w') # 'a' to append. 'w' to (over)write\n",
    "print(words, file=fout)\n",
    "fout.close() # tidy up!!\n"
   ]
